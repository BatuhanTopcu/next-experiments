{
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "printWidth": 100,
  "endOfLine": "lf",
  "importOrder": [
    "server-only",
    "<THIRD_PARTY_MODULES>",
    "^@/(?!components)(.*)$",
    "^@/components/ui/(.*)$",
    "^@/components/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
